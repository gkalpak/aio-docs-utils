{
  "name": "aio-docs-utils",
  "displayName": "Angular.io Documentation Utilities",
  "description": "A VSCode extension providing a set of simple utilities to aid in authoring/viewing documentation for angular.io.",
  "version": "0.0.0",
  "publisher": "gkalpak",
  "repository": "github:gkalpak/aio-docs-utils",
  "license": "Unlicense",
  "main": "./out/extension",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project .",
    "clean": "node --eval \"require('shelljs').rm('-rf', 'out');\"",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "pretest": "npm run build",
    "test": "node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "npm run build",
    "watch": "npm run build -- --watch"
  },
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.26.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^7.0.65",
    "jasmine": "^3.1.0",
    "mock-require": "^3.0.2",
    "shelljs": "^0.8.2",
    "source-map-support": "^0.5.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "vscode": "^1.1.18"
  },
  "private": true
}
